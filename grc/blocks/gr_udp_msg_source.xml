<?xml version="1.0"?>
<!--
###################################################
##Message Sink
###################################################
 -->
<block>
	<name>UDP Message Source</name>
	<key>gr_udp_msg_source</key>
	<import>from gnuradio import gr</import>
	<make>gr.udp_msg_source(port=$port, msgq=$(id)_msgq_out)</make>
	<param>
		<name>UDP Port</name>
		<key>port</key>
		<value>1234</value>
		<type>int</type>
	</param>
	<source>
		<name>out</name>
		<type>msg</type>
	</source>
</block>
