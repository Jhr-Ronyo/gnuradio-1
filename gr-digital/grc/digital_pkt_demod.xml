<block>
	<name>Packet Demod</name>
	<key>digital_pkt_demod</key>
	<import>from gnuradio.digital import pkt</import>
	<make>pkt.demod_pkts(access_code=None, msgq_out=$(id)_msgq_out, threshold=-1)</make>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>msg</type>
	</source>
</block>
