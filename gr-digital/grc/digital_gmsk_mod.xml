<!--
###################################################
##GMSK Mod
###################################################
 -->
<block>
	<name>GMSK Mod</name>
	<key>digital_gmsk_mod</key>
	<import>from gnuradio.digital import gmsk</import>
	<make>gmsk.gmsk_mod(samples_per_symbol=$samples_per_symbol,
                                    bt=$bt, verbose=False, log=False)
	</make>
	<param>
		<name>Samples per symbol</name>
		<key>samples_per_symbol</key>
		<value>2</value>
		<type>int</type>
	</param>
	<param>
		<name>BT</name>
		<key>bt</key>
		<value>0.35</value>
		<type>real</type>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>complex</type>
	</source>
</block>
